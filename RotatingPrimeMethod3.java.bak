class RotatingPrimeMethod3 
{
	public static void main(String[] args) 
	{
       rotatingPrime(131);
	}
	
	
	public static void allRotations(int num)
	{
		int rot = num;
		
		do
		{
			System.out.println(rot);
			rot = rotate(rot);
		}
		while (rot!=num);
	}
	
	public static boolean rotatingPrime(int num)
	{
		int rot = num;
		
		do
		{
			if(!isPrime(rot))
			{
				return false;
			}
			rot = rotate(rot);
		}
		while (rot!=num);
		return true;
	}
	
	public static boolean isPrime(int num)
	{
		int den = 2;
		while (den<num)
		{
			if(num%den == 0)
			{
				break;
			}
			den++;
		}
			
			return num == den;
		}
	
	
	
	public static int rotate(int num)
	{
		return num%10*power(10, count(num)-1)+num/10;
	}
	
	public static int power(int base, int raise)
	{
		int pow = 1;
		
		while (raise>0)
		{
			pow*=base;
			raise--;
		}
		return pow;
	}
	
	public static int count(int num)
	{
		int ct = 0;
		
		for (;num!=0 ;num/=10 )
		{
			ct++;
		}
		return ct;
	}
	
}
class Emirp 
{
	public static void main(String[] args) 
	{
		System.out.println("Hello World!");
	}
	
	public static boolean isEmirp(int num)
	{
		int rev = reverse(num);
		return num!=rev && isPrime(num) && isPrime(rev);
	}
	
	public static boolean isPrime(int num)
	{
		int den = 2;
		while(den<num)
		{
			if(num%den == 0)
			{
				return false;
			}
			den++;
		}
		return true;
	}
	
	public static int reverse(int num)
	{
		int rev = 0;
		while(num!=0)
		{
			rev = rev*10 + num%10;
			num/=10;
		}
		return rev;
	}
}


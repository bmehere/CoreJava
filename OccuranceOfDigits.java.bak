//calculate the occurence of given digit in a number


class OccuranceOfDigits
{
	public static void main(String[] args) 
	{
     int num = 2327972;
	 int digit = 2;
	 int count = 0;
	 int last = 0;
	 
	 if(digit >= 0 && digit<9)
		{
		 while (num>0)
		 {
			  last = num%10;
		 }
		 if (last == digit)
		 {
			 count++;
		 }
		 num/=10;
	}
}
 System.out.println(count);
}
